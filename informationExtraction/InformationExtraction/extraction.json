[{"uuid": 0, "content": "纽约造船公司于1941年12月29日在新泽西州卡姆登市建立了代顿（CL-78）", "date": ["1941年12月29日"], "relationEntity": [{"uuid": 0, "name": "纽约", "label": "地点"}, {"uuid": 1, "name": "造船公司", "label": "候选"}, {"uuid": 5, "name": "新泽西州", "label": "地点"}, {"uuid": 6, "name": "卡姆", "label": "人物"}, {"uuid": 7, "name": "登市", "label": "地点"}, {"uuid": 10, "name": "代顿", "label": "武器"}, {"uuid": 12, "name": "CL", "label": "候选"}]}, {"uuid": 1, "content": "于1942年3月27日重新分类为CV-26", "date": ["1942年3月27日"], "relationEntity": [{"uuid": 5, "name": "CV", "label": "候选"}]}, {"uuid": 2, "content": "于1942年3月31日更名为蒙特利", "date": ["1942年3月31日"], "relationEntity": [{"uuid": 4, "name": "蒙特利", "label": "人物"}]}, {"uuid": 3, "content": "于1943年2月28日下水", "date": ["1943年2月28日"], "relationEntity": [{"uuid": 2, "name": "下水", "label": "候选"}]}, {"uuid": 4, "content": "由P.N.L.贝林格夫人赞助", "date": [], "relationEntity": [{"uuid": 2, "name": "N", "label": "候选"}, {"uuid": 5, "name": "夫人", "label": "候选"}]}, {"uuid": 5, "content": "并于1943年6月17日委任莱斯特T.亨特上尉指挥", "date": ["1943年6月17日"], "relationEntity": [{"uuid": 4, "name": "莱斯特T.亨特", "label": "地点"}, {"uuid": 5, "name": "上尉", "label": "候选"}]}, {"uuid": 6, "content": "蒙特利号于1943年7月15日重新定级为CVL-26，在服役不久后，在安定下来后，离开费城前往西太平洋", "date": ["1943年7月15日"], "relationEntity": [{"uuid": 0, "name": "蒙特利", "label": "人物"}, {"uuid": 7, "name": "CVL-26", "label": "候选"}, {"uuid": 20, "name": "费城", "label": "地点"}, {"uuid": 22, "name": "西太平洋", "label": "地点"}]}, {"uuid": 7, "content": "她于1943年11月19日到达吉尔伯茨号，及时帮助马金岛安全", "date": ["1943年11月19日"], "relationEntity": [{"uuid": 4, "name": "吉尔伯茨号", "label": "武器"}, {"uuid": 8, "name": "马金岛", "label": "地点"}]}, {"uuid": 8, "content": "作为TG 37.2的一部分，她参加了12月25日对新爱尔兰Kavieng的罢工，并支持在Kwajalein和Eniwetok降落，直到1944年2月8日", "date": ["12月25日", "1944年2月8日"], "relationEntity": [{"uuid": 10, "name": "新爱尔兰", "label": "地点"}, {"uuid": 11, "name": "Kavieng", "label": "候选"}, {"uuid": 18, "name": "Kwajalein", "label": "候选"}, {"uuid": 20, "name": "Eniwetok", "label": "候选"}, {"uuid": 21,